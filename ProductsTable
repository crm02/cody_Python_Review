import sqlite3

def main():

    # Creates an object that connects to a database and connects it to 'conn', if database didn't exist, it now does
    conn = sqlite3.connect('chocolate.db')

    # Creates a cursor for manipulate our database
    cur = conn.cursor()

    # Creates our table and columns
    cur.execute('''CREATE TABLE IF NOT EXISTS Products(ProductID INTEGER PRIMARY KEY NOT NULL, 
                DESCRIPTION TEXT,
                UnitCost REAL,
                RetailPrice REAL,
                UnitsOnHand INTEGER)''')
    
    # Inserts rows and values into our table
    cur.execute('''INSERT INTO Products(DESCRIPTION, UnitCost, RetailPrice, UnitsOnHand)
                VALUES('Dark Choclate Bar', 2.99, 5.99, 197),
                ('Medium Dark Chocolate Bar', 2.99, 5.99, 406),
                ('Milk Chocolate Bar', 2.99, 5.99, 266),
                ('Chocolate Truffles', 5.99, 11.99, 398),
                ('Chocolate Caramel Bar', 3.99, 6.99, 272),
                ('Choclate Raspberry Bar', 3.99, 6.99, 363),
                ('Chocolate and Cashew Bar', 4.99, 9.99, 325),
                ('Hot Chocolate Mix', 5.99, 12.99, 222),
                ('Semisweet Chocolate Chips', 1.99, 3.99, 163),
                ('White Chocolate Chips', 1.99, 3.99, 293)''')
    
