import sqlite3

def main():

    # Creates an object that connects to a database and connects it to 'conn', if database didn't exist, it now does
    conn = sqlite3.connect('chocolate.db')

    # Creates a cursor for manipulate our database
    cur = conn.cursor()

    # Creates our table and columns
    cur.execute('''CREATE TABLE IF NOT EXISTS Products(ProductID INTEGER PRIMARY KEY, 
                DESCRIPTION TEXT,
                UnitCost REAL,
                RetailPrice REAL,
                UnitsOnHand INTEGER)''')
    
    # Inserts rows and values into our table
    cur.execute('''INSERT INTO Products(DESCRIPTION, UnitCost, RetailPrice, UnitsOnHand)
                VALUES('Dark Choclate Bar', 2.99, 5.99, 197),
